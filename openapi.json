components:
  schemas:
    Feedback:
      type: object
      properties:
        id:
          type: integer
          readOnly: true
        ref_date:
          type: string
          format: date
        scope:
          type: string
          maxLength: 20
        topic:
          type: string
          maxLength: 30
        message:
          type: string
        created_at:
          type: string
          format: date-time
          readOnly: true
        user:
          type: integer
      required:
      - created_at
      - id
      - message
